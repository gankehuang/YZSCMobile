.page-wrapper {
  padding-top: 65px;
}
.form_wrapper {
  padding: 0 30rpx;
}
.editor-title {
  height: 80rpx;
}
.editor-title .img {
    line-height: 80rpx;
}
.editor-title .img image {
      width: 12rpx;
      height: 12rpx;
      vertical-align: middle;
}
.editor-title text {
    margin-left: 14rpx;
    line-height: 80rpx;
    font-size: 32rpx;
}
.editor-title .confirm-btn {
    width: 140rpx;
    height: 54rpx;
}
.editor-title .confirm-btn button {
      width: 140rpx;
      height: 54rpx;
      font-size: 24rpx;
      padding: 0;
      background-color: #4684EA;
}
.editor-title.form-seat {
    height: 120rpx;
}
.card-form .form-list {
  height: 96rpx;
  border-bottom: 1rpx solid #EBEEF2;
  font-size: 28rpx;
}
.card-form .form-list .field text {
    color: #FF6354;
    margin-left: 10rpx;
}
.card-form .form-list .s-value {
    color: #B2B2B2;
    vertical-align: middle;
}
.card-form .form-list .s-value.d-num {
      text-align: right;
}
.card-form .form-list .s-value.d-num input {
        font-size: 14px;
}
.submits {
  margin-top: 90rpx;
  margin-bottom: 40rpx;
}
.submits button {
    width: 316rpx;
    height: 88rpx;
}
.submits button.editor-btn {
      background-color: #F05E57;
}
.submits button.submit-btn {
      background-color: #4684EA;
}
.editor_wraper .thead_row {
  background-color: #70ADFF;
  color: white;
  height: 32px;
  font-size: 12px;
}
.editor_wraper .tbody_row {
  background-color: #EDF3F9;
  height: 32px;
  padding: 0 6px;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  color: #4D4D4D;
}
.editor_wraper .tbody_row:nth-child(even) {
    background-color: white;
}
.editor_wraper .tbody_row .input_inner {
    border: 1px solid #E2E2E2;
    border-radius: 2px;
    margin: 0 2px;
    font-size: 12px;
    height: 46rpx;
    background-color: white;
    padding: 0 4px;
}
.center {
  text-align: center;
}

