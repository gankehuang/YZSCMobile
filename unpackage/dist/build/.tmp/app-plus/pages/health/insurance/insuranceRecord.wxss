.health-container {
  background-color: #F5F5F5;
  padding: 0 30rpx 120rpx;
}
.health-container .editor-title {
    height: 44rpx;
}
.health-container .editor-title .img {
      line-height: 44rpx;
}
.health-container .editor-title .img image {
        width: 12rpx;
        height: 12rpx;
        vertical-align: middle;
}
.health-container .editor-title text {
      margin-left: 14rpx;
      line-height: 44rpx;
      font-size: 32rpx;
}
.health-container .card-form {
    margin-top: 0rpx;
}
.health-container .card-form .input-style {
      color: #B2B2B2;
      text-align: right;
}
.health-container .card-form .input-style .uni-input-wrapper .uni-input-placeholder {
      color: #B2B2B2;
      text-align: right;
      font-size: 28rpx;
}
.health-container .editor-title {
    height: 80rpx;
}
.health-container .editor-title .confirm-btn {
      width: 140rpx;
      height: 54rpx;
}
.health-container .editor-title .confirm-btn button {
        width: 140rpx;
        height: 54rpx;
        font-size: 24rpx;
        padding: 0;
        background-color: #4684EA;
}
.health-container .editor-title.form-seat {
      height: 120rpx;
}
.health-container .submitData {
    margin-top: 32rpx;
}
.health-container .submitData .cancel-btn {
      width: 316rpx;
      background-color: #F05E57;
      color: #FFFFFF;
}
.health-container .submitData .submit-btn {
      width: 316rpx;
      background-color: #4684EA;
      color: #FFFFFF;
}

