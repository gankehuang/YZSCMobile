<view class="health-container"><view class="editor-title flex ali-c"><view class="img"><image src="/static/assets/rect.png" mode></image><text>基本信息</text></view></view><view class="card-form card"><uni-cell vue-id="1" title="猪场" value="{{form.name}}" bind:__l="__l"></uni-cell><uni-cell vue-id="2" title="分场" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="3" title="业务日期" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="4" title="免疫类型" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="5" title="调整类型" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="6" title="调整原因" required="true" bind:__l="__l" vue-slots="{{['value']}}"><view class="input-style" slot="value"><input placeholder="请输入原因" data-event-opts="{{[['input',[['__set_model',['','getNumber','$event',[]]]]]]}}" value="{{getNumber}}" bindinput="__e"/></view></uni-cell><uni-cell vue-id="7" title="推迟天数" bind:__l="__l" vue-slots="{{['value']}}"><view class="input-style" slot="value"><input placeholder="请输入天数" data-event-opts="{{[['input',[['__set_model',['','dayNumber','$event',[]]]]]]}}" value="{{dayNumber}}" bindinput="__e"/></view></uni-cell></view><view class="editor-title flex ali-c"><view class="img"><image src="/static/assets/rect.png" mode></image><text>调整明细</text></view></view><view class="data__wrapper"><ztable vue-id="8" tableData="{{tableData}}" columns="{{columns}}" emptyText="-" data-event-opts="{{[['^rowTap',[['rowTapHandler']]],['^checkbox',[['checkbox']]]]}}" bind:rowTap="__e" bind:checkbox="__e" bind:__l="__l"></ztable></view><view class="submits jus-b"><button class="flexc submit-btn" type="primary">提交</button></view><popup-layer class="vue-ref" vue-id="9" direction="right" data-ref="popupRef" bind:__l="__l" vue-slots="{{['default']}}"><list-page bind:selectPages="__e" vue-id="{{('10')+','+('9')}}" data-event-opts="{{[['^selectPages',[['selectPages']]]]}}" bind:__l="__l"></list-page></popup-layer></view>