.pages {
  width: 500rpx;
}
.pages .title-head {
    height: 100rpx;
    padding-left: 20rpx;
    font-size: 34rpx;
    font-weight: bold;
    color: #4D4D4D;
    border-bottom: 1rpx solid #f5f5f5;
}
.pages .page-options {
    height: 80rpx;
    padding-left: 10px;
    border-bottom: 1rpx solid #f5f5f5;
}
.pages .page-options .option {
      padding: 0 10rpx;
      height: 100%;
      font-size: 28rpx;
      color: #7A7A7A;
}
.pages .page-options .option.on {
        color: #3A75E7;
}
.pages .pages-list {
    padding: 20rpx;
}
.pages .pages-list .pages {
      width: 100rpx;
      height: 52rpx;
      font-size: 24rpx;
      border: 1rpx solid #f5f5f5;
      border-radius: 52rpx;
      color: #7A7A7A;
      margin: 10rpx 6rpx;
}
.pages .pages-list .pages.check {
        background-color: #3A75E7;
        color: #fff;
}

