<view class="semen-editor"><view class="status"><image class="status-bg" src="/static/assets/top_bg@2x.png" mode="widthFix"></image><view class="status-header"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="back-icon flexc ali-c" bindtap="__e"><image class="icon" src="/static/assets/back.png"></image></view><view class="status-title">空瓶回收编辑</view></view></view><view class="main-container"><view class="card-form card"><uni-cell vue-id="1" title="分场" value="{{form.name}}" bind:__l="__l"></uni-cell><uni-cell vue-id="2" title="免疫项目" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="3" title="免疫计划" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="4" title="疫苗名称" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="5" title="领用日期" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="6" title="领用数量" required="true" bind:__l="__l" vue-slots="{{['value']}}"><view class="input-style" slot="value"><input type="number" placeholder="请输入数量" data-event-opts="{{[['input',[['__set_model',['','getNumber','$event',[]]]]]]}}" value="{{getNumber}}" bindinput="__e"/></view></uni-cell><uni-cell vue-id="7" title="领用单位" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="8" title="空瓶回收批号" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="9" title="空瓶回收数量" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell vue-id="10" title="执行人" value="{{form.projectName}}" bind:__l="__l"></uni-cell><uni-cell class="card-cell-last" vue-id="11" title="执行状态" value="{{form.projectName}}" bind:__l="__l"></uni-cell></view><view class="submits jus-b"><button class="flexc submit-btn" type="primary">提交</button></view></view></view>