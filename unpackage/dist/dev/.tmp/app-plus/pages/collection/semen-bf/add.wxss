page {
  background: #f2f5fc;
}
.dis {
  display: none;
}
.status_bar {
  height: 128rpx;
  width: 100%;
  position: fixed;
  z-index: 99999;
}
.status_bar .status {
    position: relative;
}
.status_bar .status .icon-back {
      position: absolute;
      top: 50%;
      margin-top: -10rpx;
      padding-left: 40rpx;
      width: 10%;
}
.status_bar .status .icon-back .icon {
        width: 16rpx;
        height: 28rpx;
}
.status_bar .status .icon-set {
      color: #fff;
      position: absolute;
      top: 50%;
      margin-top: -10rpx;
      right: -10rpx;
      width: 20%;
}
.status_bar .status .icon-set .icon2 {
        width: 8rpx;
        height: 42rpx;
}
.status_bar .status__bg {
      width: 100%;
}
.status_bar .status__title {
      font-family: PingFangSC-Semibold;
      font-size: 32rpx;
      color: #FFFFFF;
      position: absolute;
      top: 50%;
      left: 50%;
      margin-top: -10rpx;
      margin-left: -85rpx;
}
.semen-editor {
  background-color: #F5F5F5;
  padding: 130rpx 30rpx 124rpx 30rpx;
}
.semen-editor .editor-title {
    height: 80rpx;
}
.semen-editor .editor-title .img {
      line-height: 80rpx;
}
.semen-editor .editor-title .img image {
        width: 12rpx;
        height: 12rpx;
        vertical-align: middle;
}
.semen-editor .editor-title text {
      margin-left: 14rpx;
      line-height: 80rpx;
      font-size: 32rpx;
}
.semen-editor .editor-title .confirm-btn {
      width: 140rpx;
      height: 54rpx;
}
.semen-editor .editor-title .confirm-btn button {
        width: 140rpx;
        height: 54rpx;
        font-size: 24rpx;
        padding: 0;
        background-color: #4684EA;
}
.semen-editor .editor-title.form-seat {
      height: 120rpx;
}
.semen-editor .editor-title .table-btn {
      margin-left: 250rpx;
}
.semen-editor .editor-title .tbtn-add {
      width: 142rpx;
      height: 54rpx;
      background-color: #3A75E7;
      font-size: 10px;
}
.semen-editor .editor-title .tbtn-del {
      width: 142rpx;
      height: 54rpx;
      background-color: #F05E57;
      font-size: 12px;
}
.semen-editor .card-form {
    height: 584rpx;
}
.semen-editor .card-form .form-list {
      height: 96rpx;
      border-bottom: 1rpx solid #EBEEF2;
      font-size: 28rpx;
}
.semen-editor .card-form .form-list .field {
        width: 200rpx;
}
.semen-editor .card-form .form-list .field text {
          color: #FF6354;
          margin-left: 10rpx;
}
.semen-editor .card-form .form-list .s-value {
        color: #B2B2B2;
        vertical-align: middle;
}
.semen-editor .card-form .form-list .s-value.d-num {
          text-align: right;
}
.semen-editor .card-form2 {
    margin-top: 20rpx;
}
.semen-editor .card-form2 .form-list {
      height: 96rpx;
      font-size: 28rpx;
}
.semen-editor .card-form2 .form-list .field {
        width: 250rpx;
}
.semen-editor .card-form2 .form-list .field text {
          color: #FF6354;
          margin-left: 10rpx;
}
.semen-editor .card-form2 .form-list .field .icon {
          width: 36rpx;
          height: 36rpx;
          margin-left: 20rpx;
}
.semen-editor .card-form2 .form-list .s-value {
        color: #B2B2B2;
        vertical-align: middle;
}
.semen-editor .card-form2 .form-list .s-value.d-num {
          text-align: right;
}
.semen-editor .data__wrapper {
    height: 584;
}
.submits {
  position: fixed;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  background: #F5F5F5;
  width: 750rpx;
  height: 124rpx;
  left: 0;
}
.submits button {
    width: 316rpx;
    height: 88rpx;
}
.submits button.editor-btn {
      background-color: #F05E57;
      margin: 20rpx 25rpx 20rpx 40rpx;
}
.submits button.submit-btn {
      background-color: #4684EA;
      margin: 20rpx 30rpx 20rpx 25rpx;
}

