<view><view class="pagination__wrapper"><view data-event-opts="{{[['tap',[['selectPage',['$event']]]]]}}" class="pagination__item" bindtap="__e">选页</view><view data-event-opts="{{[['tap',[['prev',['$event']]]]]}}" class="pagination__item" bindtap="__e"><image class=" icon" src="/static/assets/prev.png"></image></view><view class="pagination__item slide__wrap"><slider class="page_slider" min="1" value="{{page}}" max="{{pageNum}}" activeColor="#3A75E7" block-size="14" data-event-opts="{{[['change',[['change',['$event']]]],['changing',[['changing',['$event']]]]]}}" bindchange="__e" bindchanging="__e"></slider></view><view data-event-opts="{{[['tap',[['next',['$event']]]]]}}" class="pagination__item" bindtap="__e"><image class="icon" src="/static/assets/next.png"></image></view></view><view hidden="{{!(showNum)}}" class="popup__pageNum"><text>{{page+"/"+pageNum}}</text></view></view>